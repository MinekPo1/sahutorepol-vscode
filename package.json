{
    "name": "sahutorepol-vscode",
    "displayName": "SaHuTOrEPoL",
    "publisher": "minekpo1",
    "description": "a language extension for the esoteric programing language SaHuTOrEPoL",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "sahutorepol",
                "aliases": [
                    "SaHuTOrEPoL",
                    "sahutorepol"
                ],
                "extensions": [
                    ".sah"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "sahutorepol",
                "scopeName": "source.sahutorepol",
                "path": "./syntaxes/sahutorepol.tmLanguage.json"
            }
        ]
    },
    "dependencies": {
        "vsce": "^2.6.3"
    },
    "repository": "https://github.com/MinekPo1/sahutorepol-vscode",
    "icon": "SaH.png"
}
