{
    "name": "sahutorepol-vscode",
    "displayName": "SaHuTOrEPoL",
    "description": "a language extention for the esoteric programing language SaHuTOrEPoL",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "sahutorepol",
            "aliases": ["SaHuTOrEPoL", "sahutorepol"],
            "extensions": [".sah"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "sahutorepol",
            "scopeName": "source.sahutorepol",
            "path": "./syntaxes/sahutorepol.tmLanguage.json"
        }]
    }
}
